<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Cuentas_a_Cobrar</fullName>
        <accessLevel>Read</accessLevel>
        <description>Ampliar la visibilidad de los reclamos para el grupo Cuentas a Cobrar</description>
        <label>Cuentas a Cobrar</label>
        <sharedTo>
            <group>Cuentas_a_Cobrar</group>
        </sharedTo>
        <booleanFilter>1 OR (2 AND 3)</booleanFilter>
        <criteriaItems>
            <field>Viene_de_Cuentas_a_Cobrar__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Reclamos</value>
        </criteriaItems>
        <criteriaItems>
            <field>Type</field>
            <operation>equals</operation>
            <value>Pagos,Saldos a favor,Cobros por debito automatico,Desconocimiento Deuda</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Presupuesto_Administracion_de_Convenios</fullName>
        <accessLevel>Read</accessLevel>
        <description>Sharing Rule para compartir registros de usuarios de Administracion de  Prestadores respecto de casos Subtipo= Valor Presupuesto o Ap Presupuesto = True- JIRA DES 5195</description>
        <label>Presupuesto Administracion de Convenios</label>
        <sharedTo>
            <group>Presupuesto_Administracion_de_Convenios</group>
        </sharedTo>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Sub_tipo__c</field>
            <operation>equals</operation>
            <value>Presupuesto</value>
        </criteriaItems>
        <criteriaItems>
            <field>AP_Presupuesto__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
